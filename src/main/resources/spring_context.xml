<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
	   	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<context:annotation-config />

	<bean class="org.ihtsdo.ts.importer.Importer"/>

	<bean class="org.ihtsdo.otf.dao.s3.S3ClientImpl" >
		<constructor-arg>
			<bean class="com.amazonaws.auth.BasicAWSCredentials">
				<constructor-arg value="${s3.key}"/>
				<constructor-arg value="${s3.secretKey}"/>
			</bean>
		</constructor-arg>
	</bean>

	<bean class="org.ihtsdo.ts.importer.jira.JiraProjectSync">
		<constructor-arg value="${jira.projectKey}"/>
		<constructor-arg value="${jira.url}"/>
		<constructor-arg value="${jira.username}"/>
		<constructor-arg value="${jira.password}"/>
	</bean>

	<bean class="org.ihtsdo.context.OrderedPropertyPlaceholderConfigurer" >
		<property name="location" value="ts-delta-import.properties"/>
	</bean>

</beans>
